<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @product.name %>
  
</p>

<ul>
  <% @product.images.each do |image| %>
    <li>
      <img src="<%=url_for(image.image)%>">
    </li>
  <% end %>
</ul>

<%= link_to 'Convert', convert_product_path(@product) %> |
<%= link_to 'Edit', edit_product_path(@product) %> |
<%= link_to 'Back', products_path %>
